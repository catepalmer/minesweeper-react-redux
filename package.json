  "private": true,
  "dependencies": {
    "apollo-server": "^0.1.5",
    "body-parser": "^1.18.3",
    "casual": "^1.5.3",
    "cors": "^2.7.1",
    "express": "^4.16.4",
    "express-graphql": "^0.7.1",
    "graphql": "^14.0.2",
    "graphql-server-express": "^1.4.0",
    "graphql-tools": "^0.4.2",
    "pg": "^7.6.1",
