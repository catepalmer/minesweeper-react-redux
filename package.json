    "babel-preset-stage-0": "6.5.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.3",
    "graphql": "^0.6.0",
    "husky": "^0.14.3",
    "jest-enzyme": "^6.0.0",
    "jest-mock-random": "^1.0.2",
    "jest-styled-components": "^5.0.1",
    "lint-staged": "^7.0.4",
