    "sequelize": "^4.41.2",
    "sinon": "^7.0.0",
    "styled-components": "^3.2.5"
  },
  "scripts": {
    "start": "react-scripts start",
    "dev": "nodemon ./packages/server/server.js --exec babel-node",
    "build": "react-scripts build",
